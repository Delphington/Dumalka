<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/FH_BtnNewQTrueSolid" />
            <stroke
                android:width="@dimen/_1sdp"
                android:color="@color/FH_BtnNewQTrueStroke" />
            <corners android:radius="@dimen/_2sdp" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/FH_BtnNewQFalseSolid" />
            <stroke
                android:width="@dimen/_1sdp"
                android:color="@color/FH_BtnNewQFalseStroke" />
            <corners android:radius="@dimen/_2sdp" />
        </shape>
    </item>

</selector>