<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/DNI_BtnBackTrueSolid" />
            <stroke android:width="@dimen/_1sdp" android:color="@color/DNI_BtnBackTrueStroke" />
            <corners android:radius="@dimen/_2sdp" />
        </shape>
    </item>


    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/DNQ_BtnBackFalseSolid" />
            <stroke android:width="@dimen/_1sdp" android:color="@color/DNI_BtnBackFalseStroke" />
            <corners android:radius="@dimen/_2sdp" />

        </shape>
    </item>


</selector>