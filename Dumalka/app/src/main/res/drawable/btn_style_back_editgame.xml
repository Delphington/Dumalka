<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/DEG_BtnBackTrueSolid" />
            <stroke android:width="2dp" android:color="@color/DEG_BtnBackTrueStroke" />
            <corners android:radius="3dp" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/DEG_BtnBackFalseSolid" />
            <stroke android:width="2dp" android:color="@color/DEG_BtnBackFalseStroke" />
            <corners android:radius="3dp" />
        </shape>
    </item>

</selector>